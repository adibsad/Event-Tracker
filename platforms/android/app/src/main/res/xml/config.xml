<?xml version='1.0' encoding='utf-8'?>
<widget id="net.nestmk12.eventtracker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Event Tracker</name>
    <description>
        An app designed by NEST+M Coding Club to consolidate all important events into one central location. If you happen to miss morning anouncments, you'll be able to easily view any upcoming Nest+M events on our app!
    </description>
    <author email="codingclub@gmail.com" href="https://nestmk12.net/">
        NEST+M
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
